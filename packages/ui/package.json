{
  "name": "enact-ui",
  "version": "1.0.0-alpha.1",
  "description": "A collection of simplified unstyled cross-platform UI components for Enact",
  "main": "index.js",
  "scripts": {
    "clean": "enact clean",
    "pack": "enact pack",
    "pack-p": "enact pack -p",
    "test": "enact test start --single-run --browsers PhantomJS",
    "test-json": "enact test start --single-run --browsers PhantomJS --reporters json",
    "test-watch": "enact test start --browsers PhantomJS",
    "transpile": "enact transpile",
    "watch": "enact pack --watch"
  },
  "homepage": "http://enyojs.com/",
  "bugs": "http://jira.enyojs.com/",
  "maintainers": [
    {
      "name": "Enyo JS Framework Team",
      "web": "http://enyojs.com/"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enyojs/enact.git"
  },
  "license": "Apache-2.0",
  "keywords": [
    "framework",
    "toolkit",
    "components",
    "mobile",
    "webOS"
  ],
  "enact": {
    "ri": {
      "baseSize": 24
    }
  },
  "eslintConfig": {
    "extends": "enact"
  },
  "devDependencies": {
    "enzyme": "^2.4.1",
    "eslint-config-enact": "enyojs/eslint-config-enact",
    "sinon": "git+https://github.com/sinonjs/sinon.git"
  },
  "dependencies": {
    "enact-core": "1.0.0-alpha.1",
    "ramda": "^0.22.1",
    "react-dom": "^15.3.1",
    "recompose": "^0.20.2",
    "warning": "^3.0.0"
  },
  "peerDependencies": {
    "enact-dev":"enyojs/enact-dev#release/0.1.0"
  }
}
