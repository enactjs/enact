{
  "name": "@enact/ui",
  "version": "2.2.7",
  "description": "A collection of simplified unstyled cross-platform UI components for Enact",
  "main": "index.js",
  "scripts": {
    "clean": "enact clean",
    "lint": "enact lint --strict",
    "test": "enact test start --single-run --browsers PhantomJS",
    "test-json": "enact test start --single-run --browsers PhantomJS --reporters json",
    "test-watch": "enact test start --browsers PhantomJS",
    "transpile": "enact transpile"
  },
  "license": "Apache-2.0",
  "keywords": [
    "framework",
    "toolkit",
    "components",
    "mobile",
    "webOS"
  ],
  "enact": {
    "ri": {
      "baseSize": 24
    }
  },
  "eslintConfig": {
    "extends": "enact/strict"
  },
  "dependencies": {
    "@enact/core": "^2.2.7",
    "classnames": "^2.2.5",
    "direction": "^1.0.1",
    "invariant": "^2.2.2",
    "prop-types": "^15.6.0",
    "ramda": "^0.24.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "recompose": "^0.26.0",
    "warning": "^3.0.0"
  }
}
