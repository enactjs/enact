{
  "name": "@enact/moonstone",
  "version": "1.3.0",
  "description": "Large-screen/TV support library for Enact, containing a variety of UI components.",
  "main": "index.js",
  "scripts": {
    "clean": "enact clean",
    "lint": "enact lint --framework",
    "test": "enact test start --single-run --browsers PhantomJS",
    "test-json": "enact test start --single-run --browsers PhantomJS --reporters json",
    "test-watch": "enact test start --browsers PhantomJS",
    "transpile": "enact transpile"
  },
  "license": "Apache-2.0",
  "keywords": [
    "framework",
    "toolkit",
    "components",
    "mobile",
    "webOS"
  ],
  "enact": {
    "ri": {
      "baseSize": 24
    }
  },
  "eslintConfig": {
    "extends": "enact/strict"
  },
  "dependencies": {
    "@enact/core": "^1.3.0",
    "@enact/i18n": "^1.3.0",
    "@enact/spotlight": "^1.3.0",
    "@enact/ui": "^1.3.0",
    "classnames": "~2.2.5",
    "eases": "~1.0.8",
    "invariant": "~2.2.1",
    "prop-types": "~15.5.0",
    "ramda": "~0.23.0",
    "react": "~15.5.0",
    "recompose": "~0.23.1",
    "warning": "~3.0.0"
  }
}
