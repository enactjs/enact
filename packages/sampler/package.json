{
  "name": "enact-sampler",
  "version": "2.0.0-alpha.2",
  "description": "Component and QA samples for Enact",
  "private": true,
  "main": "index.js",
  "scripts": {
    "lint": "enact lint --framework",
    "test": "echo \"No test suite\" && exit 0",
    "pack": "build-storybook -c .storybook -o dist",
    "pack-qa": "build-storybook -c .qa-storybook -o dist",
    "serve": "start-storybook -p 8080 -c .storybook",
    "serve-qa": "start-storybook -p 8080 -c .qa-storybook"
  },
  "license": "Apache-2.0",
  "eslintConfig": {
    "extends": "enact/strict"
  },
  "dependencies": {
    "@enact/core": "next",
    "@enact/i18n": "next",
    "@enact/moonstone": "next",
    "@enact/spotlight": "next",
    "@enact/ui": "next",
    "@enact/webos": "next",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "@enact/dev-utils": "^0.4.0",
    "@storybook/addon-actions": "^3.2.17",
    "@storybook/addon-info": "^3.2.18",
    "@storybook/addon-knobs": "^3.2.18",
    "@storybook/addon-options": "^3.2.17",
    "@storybook/react": "^3.2.17",
    "autoprefixer": "^7.2.3",
    "babel-core": "^6.26.0",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.9",
    "resolution-independence": "0.0.3",
    "style-loader": "^0.19.1"
  }
}
